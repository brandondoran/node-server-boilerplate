{
  "name": "node-server-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Node Express App using Babel",
  "main": "index.js",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/brandondoran/node-server-boilerplate.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint . --ext .js || true",
    "pretest": "npm run lint",
    "test": "env NODE_ENV=test ./node_modules/.bin/mocha --recursive --reporter spec --require babel-register",
    "start": "node server"
  },
  "keywords": [
    "node",
    "express",
    "babel",
    "eslint",
    "mocha",
    "boilerplate"
  ],
  "author": "Brandon Doran <bdoran@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "mocha": "^2.3.4",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "body-parser": "^1.14.2",
    "compression": "^1.6.1",
    "convict": "^1.0.2",
    "express": "^4.13.4"
  }
}
